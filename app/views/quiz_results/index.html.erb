<h1>Quiz Results</h1>
hello!

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quiz_results.each do |quiz_result| %>
      <tr>
        <td><%= link_to 'Show', student_quiz_result_path(quiz_result, student_id: quiz_result.student_id) %></td>
        <td><%= link_to 'Edit', edit_student_quiz_result_path(quiz_result, student_id: quiz_result.student_id) %></td>
        <td><%= link_to 'Destroy', student_quiz_result_path(quiz_result, student_id: quiz_result.student_id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

